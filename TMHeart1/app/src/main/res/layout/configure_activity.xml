						android:layout_marginEnd="@dimen/configure_activity_tvsex_tab_bar_margin_end"
						android:layout_marginStart="@dimen/configure_activity_tvsex_tab_bar_margin_start"
						android:layout_marginTop="@dimen/configure_activity_tvsex_tab_bar_margin_top"
						android:background="@drawable/configure_activity_tvsex_tab_bar_background"
