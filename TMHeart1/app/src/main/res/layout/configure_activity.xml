						tools:layout_editor_absoluteY="61dp"/>

					<EditText
						android:id="@+id/tvname_edit_text"
						android:layout_width="0dp"
						android:layout_height="@dimen/configure_activity_tvname_edit_text_height"
						android:layout_marginEnd="@dimen/configure_activity_tvname_edit_text_margin_end"
