						android:layout_height="@dimen/configure_activity_tvheight_edit_text_height"
						android:layout_marginEnd="@dimen/configure_activity_tvheight_edit_text_margin_end"
						android:layout_marginStart="@dimen/configure_activity_tvheight_edit_text_margin_start"
