						<com.google.android.material.tabs.TabItem
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/configure_activity_tvsex_tab_bar_tab_item_1_title"/>

						<com.google.android.material.tabs.TabItem
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="@string/configure_activity_tvsex_tab_bar_tab_item_2_title"/>

						<com.google.android.material.tabs.TabItem
							android:layout_width="wrap_content"
