					android:layout_marginTop="20dp"
					android:background="@drawable/configure_activity_btnsavedetails_button_ripple"
					android:fontFamily="@font/font_roboto_regular"
					android:gravity="center"
					android:text="@string/configure_activity_btnsavedetails_button_text"
					android:textColor="@color/configure_activity_btnsavedetails_button_text_color"
					android:textSize="@dimen/configure_activity_btnsavedetails_button_text_size"
